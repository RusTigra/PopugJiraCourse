{
  "ConnectionStrings": {
    "DatabaseContext": "Server=(localdb)\\mssqllocaldb;Database=PopugJira;Trusted_Connection=True;MultipleActiveResultSets=False"
  },
    "Serilog": {
    "Properties": {
      "Application": "task-board"
    },
    "MinimumLevel": {
      "Default": "Debug"
    },
    "Using": [
      "Serilog.Sinks.Async",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "Enrich": [
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "WriteTo": [
      { "Name": "Console" }
    ]
  }
}
